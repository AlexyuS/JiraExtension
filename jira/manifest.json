{
  "name": "my first extension",
  "version": "1.0.0",
  "description": "This is my very first extension",
  "manifest_version": 3,
  "permissions": ["bookmarks", "tabs", "storage"],
  "content_scripts": [
    {
      "js": ["content.js","jquery-3.6.0.js"],
      "matches": ["https://www.linkedin.com/in/*"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
